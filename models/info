Address:{
    id - id челика
    address_index - почтовый индекс - цифры
    city - город, строка
    street - улица, строка
    house - дом, цифры
    house_index - индекс дома, строка
    flat_number - номер кв, цифры
    }

Contact:{
    id - id челика
    phone - телефон, строка
    email - почта, строка
    social - социалка, строка ( линк)
    skype - скайп, строка
    telegram - телега, строка
    other_name - другое, название, строка,
    other_link - другое, ссылка, строка
}

